{
  "manifest_version": 2,
  "name": "Github Extension",
  "version": "0.1.3",
  "background": {
    "scripts": ["main.js"]
  },
  "content_scripts": [
    {
      "matches": [
         "https://github.com/*/*/issues","https://github.com/*/*/issues?q=is%3Aissue+is%3Aclosed"
      ],
      "js": ["main.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.jpg"
  },
  "permissions": [
    "http://www.api.github.com/*"
  ]
}